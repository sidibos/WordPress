class ErrorHandlerv5TestSuite extends PHPUnit_Framework_TestSuite {

	public static function suite() {
        $suite = new ErrorHandlerv5TestSuite();

		$dir = dirname(__FILE__);

		$suite->addTestFiles(array(
			$dir.'/errorHandlerTest'
		));

		return $suite;
    }

    protected function setUp() {
    }

    protected function tearDown() {
    }
}
