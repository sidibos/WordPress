<?php
/**
 * A mock HTTPRequest class which can be extended for testing
 *
 * @codingstandard ftlabs-phpcs
 * @copyright The Financial Times Limited [All Rights Reserved]
 */

class HTTPRequestV1 {
	public $url, $sent = false, $cliEquiv = null;
	protected function __construct($url) {
		$this->url = $url;
	}
	protected function send() {
		$this->sent = true;
	}
	protected function getCliEquiv() {
		return $this->cliEquiv;
	}
}
