class TemplateManagerv4TestSuite extends PHPUnit_Framework_TestSuite {

	public static function suite() {
        $suite = new TemplateManagerv4TestSuite();

		$dir = dirname(__FILE__);

		$suite->addTestFiles(array(
			$dir.'/templateManagerv4Test'
		));

		return $suite;
    }

    protected function setUp() {
    }

    protected function tearDown() {
    }
}